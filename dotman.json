{
  "repository": "atsushi130/dotfiles",
  "init": "/sync.sh",
  "dotfiles": [
    {
      "name": "vimrc",
      "input": "/vim/.vimrc",
      "output": "~/.vimrc",
      "chain": [
        "ideavimrc", "xvimrc"
      ]
    },
    {
      "name": "zshrc",
      "input": "/zsh/.zshrc",
      "output": "~/.zshrc"
    },
    {
      "name": "aliasrc",
      "input": "/.aliasrc",
      "output": "~/.aliasrc"
    },
    {
      "name": "gitconfig",
      "input": "/.gitconfig",
      "output": "~/.gitconfig"
    },
    {
      "name": "gitignore",
      "input": "/gitignore/.gitignore",
      "output": "~/.gitignore"
    },
    {
      "name": "sshrc",
      "input": "/.sshrc",
      "output": "~/.sshrc"
    },
    {
      "name": "ideavimrc",
      "input": "/vim/.ideavimrc",
      "output": "~/.ideavimrc"
    },
    {
      "name": "vimniumrc",
      "input": "/vim/.vimniumrc",
      "output": "~/.vimniumrc"
    },
    {
      "name": "xvimrc",
      "input": "/vim/.xvimrc",
      "output": "~/.xvimrc"
    },
    {
      "name": "tmux.conf",
      "input": "/tmux/.tmux.conf",
      "output": "~/.tmux.conf"
    }
  ]
}